<routes>
    <!-- Vehicle Type -->
    <vType id="car" accel="2.6" decel="4.5" length="5" maxSpeed="70" />

    <!-- Define Routes -->
    <route id="route_N_S" edges="N_to_C C_to_S" />
    <route id="route_S_N" edges="S_to_C C_to_N" />
    <route id="route_E_W" edges="E_to_C C_to_W" />
    <route id="route_W_E" edges="W_to_C C_to_E" />

    <!-- Spawn one vehicle on each of the 8 lanes -->
    <vehicle id="NS_0" type="car" route="route_N_S" depart="0" departLane="0" />
    <vehicle id="NS_1" type="car" route="route_N_S" depart="0.5" departLane="1" />

    <vehicle id="SN_0" type="car" route="route_S_N" depart="1" departLane="0" />
    <vehicle id="SN_1" type="car" route="route_S_N" depart="1.5" departLane="1" />

    <vehicle id="EW_0" type="car" route="route_E_W" depart="2" departLane="0" />
    <vehicle id="EW_1" type="car" route="route_E_W" depart="2.5" departLane="1" />

    <vehicle id="WE_0" type="car" route="route_W_E" depart="3" departLane="0" />
    <vehicle id="WE_1" type="car" route="route_W_E" depart="3.5" departLane="1" />
</routes>